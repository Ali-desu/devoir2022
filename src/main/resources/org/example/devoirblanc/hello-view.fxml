<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="449.0" prefWidth="600.0" spacing="15.0" alignment="CENTER"
      style="-fx-padding: 20; -fx-background-color: #f5f5f5; -fx-border-radius: 10; -fx-border-color: #dcdcdc;"
        >
    <children>
        <Label prefHeight="17.0" prefWidth="204.0" text="Ajouter un nouveau membre"
               style="-fx-font-size: 18px; -fx-font-weight: bold; -fx-text-fill: #333333;" />
        <TextField promptText="Nom"
                   style="-fx-font-size: 14px; -fx-padding: 8px; -fx-border-color: #dcdcdc; -fx-border-radius: 5;" />
        <TextField promptText="Prénom"
                   style="-fx-font-size: 14px; -fx-padding: 8px; -fx-border-color: #dcdcdc; -fx-border-radius: 5;" />
        <TextField promptText="Email"
                   style="-fx-font-size: 14px; -fx-padding: 8px; -fx-border-color: #dcdcdc; -fx-border-radius: 5;" />
        <TextField promptText="Téléphone"
                   style="-fx-font-size: 14px; -fx-padding: 8px; -fx-border-color: #dcdcdc; -fx-border-radius: 5;" />
        <Button mnemonicParsing="false" text="Insérer"
                style="-fx-background-color: #4caf50; -fx-text-fill: white; -fx-font-weight: bold; -fx-padding: 10px; -fx-border-radius: 5; -fx-background-radius: 5;" />
    </children>
</VBox>
